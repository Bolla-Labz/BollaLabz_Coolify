{
  "name": "bollalabz-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "BollaLabz Command Center - Frontend",
  "author": "BollaLabz",
  "license": "MIT",
  "scripts": {
    "dev": "npx vite",
    "build": "tsc && npx vite build",
    "build:no-typecheck": "npx vite build",
    "start": "node serve-frontend.js",
    "start:caddy": "caddy run --config Caddyfile --adapter caddyfile",
    "build:analyze": "tsc && npx vite build --mode analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "preview": "npx vite preview",
    "typecheck": "tsc --noEmit",
    "security:check": "node scripts/security-check.js",
    "security:audit": "npm audit --production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:production": "playwright test --config=playwright.production.config.ts tests/production/monitoring-only.spec.ts",
    "test:production:health": "playwright test --config=playwright.production.config.ts tests/production/health-check.spec.ts",
    "test:firecrawl": "tsx tests/firecrawl-test.ts",
    "test:all": "npm run test:firecrawl && npm run test:e2e",
    "docker:build": "docker build --target production -t bollalabz/frontend:latest .",
    "docker:build:node": "docker build --target node-server -t bollalabz/frontend:node .",
    "docker:run": "docker run -d -p 8080:80 --name bollalabz-frontend bollalabz/frontend:latest",
    "docker:run:node": "docker run -d -p 8080:8080 --name bollalabz-frontend-node bollalabz/frontend:node",
    "docker:stop": "docker stop bollalabz-frontend",
    "docker:clean": "docker rm -f bollalabz-frontend bollalabz-frontend-node || true",
    "docker:logs": "docker logs -f bollalabz-frontend",
    "docker:compose:up": "docker-compose -f docker-compose.frontend.yml up -d",
    "docker:compose:down": "docker-compose -f docker-compose.frontend.yml down",
    "docker:compose:logs": "docker-compose -f docker-compose.frontend.yml logs -f"
  },
